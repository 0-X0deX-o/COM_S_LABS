# Lab Week 11 - Exercise #1

print('This program takes 4 integer intputs and returns each comination of operations between the four integers...')
print('From the "+" adddition operation, the "-" subtraction operation and the "*" multiplication operation')
a = int(input("Enter the 'a' integer: "))
b = int(input("Enter the 'b' integer: "))
c = int(input("Enter the 'c' integer: "))
d = int(input("Enter the 'd' integer: "))

def AAS(a,b,c,d):
    return f'AAS: {a+b+c-d}'

def AAA(a,b,d,c):
    return f'AAA: {a+b+c+d}'

def AAM(a,b,c,d):
    return f'AAM: {a+b+c*d}'

def ASS(a,b,c,d):
    return f'ASS: {a+b-c-d}'

def ASA(a,b,c,d):
    return f'ASA: {a+b-c+d}'

def ASM(a,b,c,d):
    return f'ASM: {a+b-c*d}'
    
def AMA(a,b,c,d):
    return f'AMA: {a+b*c+d}'

def AMS(a,b,c,d):
    return f'AMS: {a+b*c-d}'
    
def AMM(a,b,c,d):
    return f'AMM: {a+b*c*d}'

def MAA(a,b,c,d):
    return f'MAA: {a*b+c+d}'
    
def MAS(a,b,c,d):
    return f'MAS: {a*b+c-d}'
    
def MAM(a,b,c,d):
    return f'MAM: {a*b+c*d}'

def MSA(a,b,c,d):
    return f'MSA: {a*b-c+d}'

def MSS(a,b,c,d):
    return f'MSS: {a*b-c-d}'

def MSM(a,b,c,d):
    return f'MSM: {a*b-c*d}'

def MMA(a,b,c,d):
    return f'MMA: {a*b*c+d}'
    
def MMS(a,b,c,d):
    return f'MMS: {a*b*c-d}'

def MMM(a,b,c,d):
    return f'MMM: {a*b*c*d}'
    
def SAA(a,b,c,d):
    return f'SAA: {a-b+c+d}'

def SAS(a,b,c,d):
    return f'SAS: {a-b+c-d}'

def SAM(a,b,c,d):
    return f'SAM: {a-b+c*d}'

def SSA(a,b,c,d):
    return f'SSA: {a-b-c*d}'

def SSS(a,b,c,d):
    return f'SSS: {a-b-c-d}'
    
def SSM(a,b,c,d):
    return f'SSM: {a-b-c*d}'

def SMA(a,b,c,d):
    return f'SMA: {a-b*c+d}'

def SMS(a,b,c,d):
    return f'SMS: {a-b*c-d}'

def SMM(a,b,c,d):
    return f'SMM: {a-b*c*d}'

def wrap(a,b,c,d):
    print(AAA(a,b,c,d))
    print(AAM(a,b,c,d))
    print(AAS(a,b,c,d))
    print(ASA(a,b,c,d))
    print(ASM(a,b,c,d))
    print(AMA(a,b,c,d))
    print(AMS(a,b,c,d))
    print(AMM(a,b,c,d))
    print(MAA(a,b,c,d))
    print(MAS(a,b,c,d))
    print(MAM(a,b,c,d))
    print(MSA(a,b,c,d))
    print(MSS(a,b,c,d))
    print(MSM(a,b,c,d))
    print(MMA(a,b,c,d))
    print(MMS(a,b,c,d))
    print(MSM(a,b,c,d))
    print(MMA(a,b,c,d))
    print(MMS(a,b,c,d))
    print(MMM(a,b,c,d))
    print(SAA(a,b,c,d))
    print(SAS(a,b,c,d))
    print(SAM(a,b,c,d))
    print(SSA(a,b,c,d))
    print(SSS(a,b,c,d))
    print(SSM(a,b,c,d))
    print(SMA(a,b,c,d))
    print(SMS(a,b,c,d))
    print(SMM(a,b,c,d))

if __name__ == "__main__":
    wrap(a,b,c,d)
